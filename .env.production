# Production environment variables
ENV=production
DEBUG=false

# Database
DATABASE_URL=postgresql+asyncpg://user:pass@host:5432/skymind

# Redis Cache
REDIS_URL=redis://default:password@host:6379/0
CACHE_ENABLED=true

# Security
ALLOWED_ORIGINS=https://skymind.ai,https://www.skymind.ai

# Monitoring (optional)
SENTRY_DSN=your_sentry_dsn_here

# API Keys (Phase 2+)
AMADEUS_CLIENT_ID=your_amadeus_id
AMADEUS_CLIENT_SECRET=your_amadeus_secret

# OpenAI (Phase 4+)
OPENAI_API_KEY=your_openai_key
